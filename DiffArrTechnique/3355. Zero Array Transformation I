    bool isZeroArray(vector<int>& arr, vector<vector<int>>& opr) {
        int n=arr.size();
        vector<int> diffA(n+1,0);
        for(auto it : opr){
            int l=it[0],r=it[1];
            diffA[l]-=1;
            diffA[r+1]+=1;
        }
        for(int i=0;i<n;i++){
            if(i!=0) diffA[i]+=diffA[i-1];
            arr[i]+=diffA[i];
            if(arr[i]>0) return false;
        }
        return true;
    }
