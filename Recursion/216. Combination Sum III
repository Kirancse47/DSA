    void solve(int i,int n,int k,vector<int> &res,vector<vector<int>> &ans){
        if(n==0 && k==0){
            ans.push_back(res);
            return;
        }
        if(i<=n && k>0 && i<10){
            res.push_back(i);
            solve(i+1,n-i,k-1,res,ans);
            res.pop_back();
            solve(i+1,n,k,res,ans);
        }
    }
    
    vector<vector<int>> combinationSum3(int k, int n) {
        vector<vector<int>> ans;
        vector<int> ds;
        solve(1,n,k,ds,ans);
        return ans;
    }
