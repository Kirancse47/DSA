    int findMin(vector<int>& nums) {
        int n=nums.size();
        int i=0,j=n-1;
        int ans=INT_MAX;
        while(i<=j){
            int m=i+(j-i)/2;
            if(nums[i]<=nums[m]){
                ans=min(ans,nums[i]);
                i=m+1;
            }else{
                ans=min(ans,nums[m]);
                j=m-1;
            }
        }
        return ans;
    }
