		vector<string> AllPossibleStrings(string s){
		    int n=s.size();
        vector<string> ans;
        for(int i=1;i<(1<<n);i++){
            string cs="";
            for(int j=0;j<n;j++){
                if(i & (1<<j)){
                    cs.push_back(s[j]);
                }
            }
            ans.push_back(cs);
        }
        sort(ans.begin(),ans.end());
        return ans;
		}
    
    Link - https://practice.geeksforgeeks.org/problems/power-set4302/1
