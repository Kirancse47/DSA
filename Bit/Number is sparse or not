    bool isSparse(int n){
        int cnt=0;
        while(n){
            if(n&1) cnt++;
            else cnt=0;
            if(cnt>=2) return false;
            n=n>>1;
        }
        return true;
    }
    
    Link - https://practice.geeksforgeeks.org/problems/number-is-sparse-or-not-1587115620/1
